{
  "name": "vika-i18n",
  "displayName": "vscode-plugin",
  "description": "vika i18n",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vika-i18n.replaceTextWithKey"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vika-i18n.replaceTextWithKey",
        "title": "Hello World"
      }
    ],
    "keybindings": [
      {
        "command": "vika-i18n.replaceTextWithKey",
        "key": "ctrl+oem_1",
        "mac": "cmd+oem_1",
        "when": "editorHasSelection"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "vika-i18n.replaceTextWithKey",
          "when": "editorHasSelection"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Vika I18N Configuration",
      "properties": {
        "vika-i18n.datasheetId": {
          "type": "string",
          "description": "i18n string è¡¨ID"
        },
        "typescript.api_token": {
          "type": "string",
          "description": "API Token"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  }
}